<h1>Editing Book</h1>
<%= form_for(@book) do |f| %>
<h4>Title</h4>
<%= f.text_field :book_title,class: "book_title"%>
<h4>Body</h4>
<%= f.text_area :book_body,class: "book_body" %>
<%= f.submit 'Update Book' %>
<p><%= link_to "Show" ,book_path(@book),class: "show_#{:id}"%></p>
<% end %>
<%= link_to "Back" ,new_book_path,class: "back"%>